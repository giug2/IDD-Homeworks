                "<td class=\"ltx_td ltx_align_center ltx_border_bb ltx_border_t\" id=\"S4.T3.1.1.6.5.5\" style=\"padding-top:2.5pt;padding-bottom:2.5pt;\">\n          0.87\n         </td>\n        \n"
