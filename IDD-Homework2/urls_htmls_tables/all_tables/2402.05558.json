                "(McMahan etÂ al., ",
                "2017",
                ")",
                ",\n",
                "\n",
                "2)",
                " ",
                "FedDFÂ ",
                "(Lin etÂ al., ",
                "2020",
                ")",
                ",\n",
                "\n",
                "3)",
                " ",
                "FedNTDÂ ",
                "(Lee etÂ al., ",
                "2021",
                ")",
                ",\n",
                "\n",
                "4)",
                " ",
                "FedProxÂ ",
                "(Li etÂ al., ",
                "2020",
                ")",
                ",\n",
                "\n",
                "5)",
                " ",
                "FedRegÂ ",
                "(Xu etÂ al., ",
                "2022",
                ")",
                ",\n",
                "\n",
                "6)",
