    "S6.T2": {
        "caption": "TABLE II: Training performance of different benchmarks.",
        "table": "<table id=\"S6.T2.4\" class=\"ltx_tabular ltx_centering ltx_guessed_headers ltx_align_middle\">\n<thead class=\"ltx_thead\">\n<tr id=\"S6.T2.4.1.1\" class=\"ltx_tr\">\n<th id=\"S6.T2.4.1.1.1\" class=\"ltx_td ltx_align_center ltx_th ltx_th_column ltx_border_t\" rowspan=\"2\"><span id=\"S6.T2.4.1.1.1.1\" class=\"ltx_text\">Policy</span></th>\n<th id=\"S6.T2.4.1.1.2\" class=\"ltx_td ltx_align_center ltx_th ltx_th_column ltx_border_t\" colspan=\"2\">MNIST (70%)</th>\n<th id=\"S6.T2.4.1.1.3\" class=\"ltx_td ltx_align_center ltx_th ltx_th_column ltx_border_t\" colspan=\"2\">CIFAR-10 (60%)</th>\n</tr>\n<tr id=\"S6.T2.4.2.2\" class=\"ltx_tr\">\n<th id=\"S6.T2.4.2.2.1\" class=\"ltx_td ltx_align_center ltx_th ltx_th_column ltx_border_t\">Final Acc.</th>\n<th id=\"S6.T2.4.2.2.2\" class=\"ltx_td ltx_align_center ltx_th ltx_th_column ltx_border_t\">Round</th>\n<th id=\"S6.T2.4.2.2.3\" class=\"ltx_td ltx_align_center ltx_th ltx_th_column ltx_border_t\">Final Acc.</th>\n<th id=\"S6.T2.4.2.2.4\" class=\"ltx_td ltx_align_center ltx_th ltx_th_column ltx_border_t\">Round</th>\n</tr>\n</thead>\n<tbody class=\"ltx_tbody\">\n<tr id=\"S6.T2.4.3.1\" class=\"ltx_tr\">\n<td id=\"S6.T2.4.3.1.1\" class=\"ltx_td ltx_align_center ltx_border_t\">Oracle</td>\n<td id=\"S6.T2.4.3.1.2\" class=\"ltx_td ltx_align_center ltx_border_t\">71.90</td>\n<td id=\"S6.T2.4.3.1.3\" class=\"ltx_td ltx_align_center ltx_border_t\">111</td>\n<td id=\"S6.T2.4.3.1.4\" class=\"ltx_td ltx_align_center ltx_border_t\">68.41</td>\n<td id=\"S6.T2.4.3.1.5\" class=\"ltx_td ltx_align_center ltx_border_t\">92</td>\n</tr>\n<tr id=\"S6.T2.4.4.2\" class=\"ltx_tr\">\n<td id=\"S6.T2.4.4.2.1\" class=\"ltx_td ltx_align_center ltx_border_t\">COCS</td>\n<td id=\"S6.T2.4.4.2.2\" class=\"ltx_td ltx_align_center ltx_border_t\">71.84</td>\n<td id=\"S6.T2.4.4.2.3\" class=\"ltx_td ltx_align_center ltx_border_t\">121</td>\n<td id=\"S6.T2.4.4.2.4\" class=\"ltx_td ltx_align_center ltx_border_t\">67.93</td>\n<td id=\"S6.T2.4.4.2.